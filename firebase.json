{
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "versatale-966fe",
          "appId": "1:777427210054:android:6d4889a5eb86aff45edf47",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "versatale-966fe",
          "configurations": {
            "android": "1:777427210054:android:6d4889a5eb86aff45edf47",
            "ios":     "1:777427210054:ios:7db1728aa0cc5fa55edf47",
            "macos":   "1:777427210054:ios:7db1728aa0cc5fa55edf47",
            "web":     "1:777427210054:web:3fabb0850a78484e5edf47",
            "windows": "1:777427210054:web:fd7a7d2bd14481d15edf47"
          }
        }
      }
    }
  },
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "/index.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, max-age=0, must-revalidate"
          }
        ]
      },
      {
        "source": "/flutter_service_worker.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, max-age=0, must-revalidate"
          }
        ]
      },
      {
        "source": "**/*.@(js|css|png|jpg|svg|json)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
